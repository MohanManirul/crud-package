{
    "name": "manirul/crudpackage",
    "description": "Simple CRUD Package for Laravel",
    "type": "library",
    "license": "MIT",
    "keywords": ["laravel", "crud", "package"],
    "authors": [
        {
            "name": "Md. Manirul Islam",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0"
    },
    "autoload": {
        "psr-4": {
            "Manirul\\CrudPackage\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Manirul\\CrudPackage\\CrudPackageServiceProvider"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}